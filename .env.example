# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# 11Labs API
ELEVEN_LABS_API_KEY=your_11labs_api_key_here

# Twilio
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER='+1234567890'
ALINA_PHONE_NUMBER='+1234567890'

# Deepgram (legacy, not used in new implementation)
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# WebSocket Configuration (for Twilio integration)
# This should be your public URL from cloudflare tunnel or ngrok
# Example: xxx.trycloudflare.com (without protocol, will be added automatically)
WEBSOCKET_URL=localhost:5001

# VAD (Voice Activity Detection) Configuration
VAD_METHOD=silero
VAD_THRESHOLD=0.5
VAD_MIN_SILENCE_MS=500
VAD_MIN_SPEECH_MS=250